{
  "id": "LIQUIDACION",
  "nombre": "Liquidación de sueldo",
  "version": "1.0",
  "clasificacion": {
    "palabras_clave": [
      "LIQUIDACION DE SUELDO",
      "LIQUIDACIÓN DE SUELDO",
      "LIQUIDACION DE REMUNERACIONES",
      "LIQUIDACIÓN DE REMUNERACIONES"
    ],
    "regex_titulo": "LIQUID(ACION|ACIÓN).*SUELDO|LIQUID(ACION|ACIÓN).*REMUNERACION(ES)?",
    "confianza_minima": 0.8
  },
  "campos": {
    "rut_trabajador": {
      "tipo": "string",
      "obligatorio": true,
      "regex": "\\d{1,2}\\.\\d{3}\\.\\d{3}-[0-9Kk]",
      "descripcion": "RUT del trabajador"
    },
    "rut_empleador": {
      "tipo": "string",
      "obligatorio": true,
      "regex": "\\d{1,2}\\.\\d{3}\\.\\d{3}-[0-9Kk]",
      "descripcion": "RUT del empleador (empresa)"
    },
    "razon_social_empleador": {
      "tipo": "string",
      "obligatorio": true,
      "descripcion": "Razón social del empleador"
    },
    "periodo_remuneracion": {
      "tipo": "string",
      "obligatorio": true,
      "regex_opciones": [
        "(?i)(ENERO|FEBRERO|MARZO|ABRIL|MAYO|JUNIO|JULIO|AGOSTO|SEPTIEMBRE|OCTUBRE|NOVIEMBRE|DICIEMBRE)\\s+del?\\s+20\\d{2}",
        "\\d{2}/20\\d{2}",
        "(?i)(ENERO|FEBRERO|MARZO|ABRIL|MAYO|JUNIO|JULIO|AGOSTO|SEPTIEMBRE|OCTUBRE|NOVIEMBRE|DICIEMBRE)\\s+20\\d{2}"
      ],
      "descripcion": "Periodo de remuneración (ej: OCTUBRE 2025 o 01/2025 → ENERO 2025)"
    },
    "total_imponible": {
      "tipo": "number",
      "obligatorio": true,
      "claves_busqueda": [
        "HABERES IMPONIBLES",
        "TOTAL IMPONIBLE",
        "Total Imponible",
        "TOTAL IMPONIBLE"
      ],
      "descripcion": "Monto total imponible"
    },
    "total_haberes": {
      "tipo": "number",
      "obligatorio": true,
      "claves_busqueda": [
        "TOTAL HABERES",
        "TOTALES",
        "TOTAL HABERES:"
      ],
      "descripcion": "Suma total de haberes"
    },
    "total_liquido": {
      "tipo": "number",
      "obligatorio": true,
      "claves_busqueda": [
        "LÍQUIDO A RECIBIR",
        "LIQUIDO A RECIBIR",
        "Liquido a pagar",
        "LIQUIDO A PAGAR",
        "ALCANCE LIQUIDO",
        "ALCANCE LÍQUIDO"
      ],
      "descripcion": "Monto líquido (incluye 'alcance líquido')"
    }
  },
  "normalizacion": {
    "montos": {
      "remover_simbolos": ["$", " "],
      "separadores_miles": [".", ","]
    },
    "periodo": {
      "map_mes_numero_a_nombre": {
        "01": "ENERO",
        "02": "FEBRERO",
        "03": "MARZO",
        "04": "ABRIL",
        "05": "MAYO",
        "06": "JUNIO",
        "07": "JULIO",
        "08": "AGOSTO",
        "09": "SEPTIEMBRE",
        "10": "OCTUBRE",
        "11": "NOVIEMBRE",
        "12": "DICIEMBRE"
      }
    }
  }
}
